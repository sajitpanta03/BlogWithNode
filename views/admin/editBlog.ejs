    <form action="/editedBlog/<%= data.id %>" method="post">

        <h1>Title</h1>
        <input type="text" name="title" value="<%= data.title %>">

        <article class="">
            <h1>Body</h1>
            <input type="text" name="body" value="<%= data.body %>">
            <button type="submit"
                style="border-radius: 10px; background-color: black; color: white; padding: 10px 20px;">submit</button>
        </article>
    </form>